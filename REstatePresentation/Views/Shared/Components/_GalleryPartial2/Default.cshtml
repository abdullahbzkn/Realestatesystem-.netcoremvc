﻿@using EntityLayer.Concrete
@model List<ServiceTerrain>

                @foreach (var item in Model)
                {
                   <a href="javascript:void(0);" onclick="window.location='/Default/ServiceTerrainDetails/'+@item.ServiceTerrainID">
                          <figure class="effect-honey tm-gallery-item">
                            <img src="@Url.Content(item.Gorsel)" alt="Image 1" class="img-fluid">
                            <figcaption>
                                <h2>
                                    <i>
                                        @item.Baslik<br /><p>@string.Format("{0:N0}", item.Fiyat) ₺ </p>
                                    </i>
                                </h2>
                            </figcaption>
                        </figure>
                    </a>
                }
               </div>
        </div>
    </div>
</div>