@using EntityLayer.Concrete
@model List<ServiceTerrain>

                @foreach (var item in Model)
                {
                     var firstPhoto = item.ServicePhotos.FirstOrDefault();
                    <a href="~/templateuser/img/gallery-img-01.jpg">
                        <figure class="effect-honey tm-gallery-item">
                              <img src="@firstPhoto?.FotografYolu" alt="Image 1" class="img-fluid">
                            <figcaption>
                                <h2><i>@item.Baslik<br />@item.Fiyat</i></h2>
                            </figcaption>
                        </figure>
                    </a>
                }
            </div>
        </div>
    </div>
</div>